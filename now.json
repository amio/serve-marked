{
  "version": 2,
  "alias": "serve-marked.now.sh",
  "regions": ["all"],
  "builds": [
    { "src": "now-index.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/now-index.js",
      "headers": {
        "cache-control": "public, max-age=360, s-maxage=2592000"
      }
    }
  ]
}
